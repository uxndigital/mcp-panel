{
  "name": "@uxndigital/mcp-panel",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "pnpm run build:api && pnpm run build:web",
    "build:api": "cd api-server && pnpm build",
    "build:web": "cd web-app && pnpm build",
    "install": "pnpm run install:api && pnpm run install:web",
    "install:api": "cd api-server && pnpm install",
    "install:web": "cd web-app && pnpm install",
    "dev": "concurrently \"cd api-server && pnpm dev\" \"cd web-app && pnpm dev\"",
    "dev:api": "cd api-server && pnpm dev",
    "dev:web": "cd web-app && pnpm dev",
    "start": "pm2 start ecosystem.config.cjs"
  },
  "devDependencies": {
    "@uxndigital/eslint-config-base": "^9.0.0",
    "@uxndigital/prettier-config": "~0.0.3",
    "concurrently": "^8.2.2",
    "eslint": "^9.29.0",
    "globals": "^16.0.0",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@9.15.9",
  "dependencies": {
    "dotenv": "^17.2.0"
  }
}